clear;clc

nFrames

x = rand(100,100,nFrames)*255;



%%

window=Screen('OpenWindow',0)


%xTexture=Screen('MakeTexture', windowPtr, x)

for i = 1:nFrames

    
       movie(i) = Screen('OpenOffscreenWindow', window, 0)  
    
frames(i) = Screen('MakeTexture', movie(i), x(:,:,i))
Screen('DrawTexture', movie(i), frames(i))


end


for i = 1:nFrames

    
    
end
Screen('Flip', windowPtr)

pause(.5)


Screen('CloseAll');